<Window x:Class="BoosterSession.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoosterSession"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="149*"/>
            <ColumnDefinition Width="651*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Background="RoyalBlue" Height="NaN" Width="NaN">
            <Button Style="{StaticResource MenuButtons}" x:Name="btnCapture" Content="Capture" Click="btnCapture_Click" />
            <Button Style="{StaticResource MenuButtons}" Name="btnFilter"  Content="Filter" Click="btnFilter_Click" />
            <Button Style="{StaticResource MenuButtons}" Name="btnStats" Content="Stats" Click="btnStats_Click" />
            <Button Style="{StaticResource MenuButtons}" Name="btnExit" Content="Exit"/>

        </StackPanel>
        <Frame Name="frmContainer" Grid.Column="1" NavigationUIVisibility="Hidden"/>


    </Grid>
</Window>
